<h1>MioVino Login</h1>
<%= form_with(url: '/sessions') do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Login" %>
<% end %>

<br><br><br>
<%= button_to "Log in with Google", '/auth/google_oauth2' %>